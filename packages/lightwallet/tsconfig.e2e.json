{
  "extends": "./tsconfig.mobile.json",
  "compilerOptions": {
    "skipLibCheck": true,
    "outDir": "./out-tsc/e2e",
    "module": "commonjs",
    "target": "es5",
    "types": [
      "jasmine",
      "jasminewd2",
      "node"
    ],
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "desktop/e2e/**/*.ts",
    "mobile/e2e/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "mobile/node_modules"
  ]
}
